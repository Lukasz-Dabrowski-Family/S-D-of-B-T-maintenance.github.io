body {
    position: relative;
    min-width: 20em;
    
    background-color: #77A240;
    
    margin-left: auto;
    margin-right: auto;
}

.main {
    visibility: hidden;    
}

.logo_frame {
	position: relative;
	float: left;
    left: 1.5%;

    width: 15%;
    height: 5em;

    border: 1px dotted darkgreen;
}

.logo {
	position: relative;
	float: left;
    left: 4%;
    top: 15%;

	width: 80%;
	height: 4em;

    background-size: 100% 4em;
	background-image: url('../images/logo.png');
	visibility: visible;
}

.frame {
    position: relative;
    top: 12.5em;
    width: 65%;
    height: 4.4em;
    
    font-family: monospace;
    font-size: 1.8em;

    color: silver;

    margin: auto;
    padding-top: 2.7%;
    text-align: center;
    
    border: 1.8px solid white;
    visibility: hidden;
}

#maintenanceTime {
    position: relative;
    display: inline-block;
    top: 1.58%;
    
    color: maroon;
    
    font-size: .9em;
    
    padding-top: 1%;
}
#interval {
    position: relative;
    display: inline-block;
    top: .7%;
}
