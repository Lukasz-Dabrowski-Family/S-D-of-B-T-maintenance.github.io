.main {
	    position: relative;
		width: 67%;
        height: 38.25em;

        border: 1px solid silver;
        box-shadow: 9px 8px 4px #506830, 0 0 4px #506830;
        
		background-color: #B8C4A3;

        font-family: verdana;

		margin-top: 3em;
        margin-left: auto;
		margin-right: auto;
		
		visibility: hidden;		
}

.lent_logo {
        position: fixed;
        z-index: 1001; 
        
        top: 6.2%; 
        right: 8%; 
        
        width: 20%;
        height: 8em;
        
	    background-image: url('../../images/egg_3.png');   
	   background-repeat: no-repeat;
}

.logo_frame {
	position: relative;
	float: left;
    left: 1.2%;
    top: 1.9%;

    width: 25%;
    height: 6em;

    border: 1px dotted darkgreen;
}

.logo {
	position: relative;
	float: left;
    left: 4%;
    top: 15%;

	width: 90%;
	height: 4em;

    background-size: 100% 4em;
	background-image: url('../../images/logo.png');
    background-repeat: no-repeat;
}

.header {
	position: relative;
    float: left;
    left: 1.2%;
    top: 5%;

    width: 97.5%;
	height: 22em;

    border: 1px dotted white;
	color: white;

    font-size: 1.1em;

    margin-left: auto;
    margin-right: auto;

	text-align: left;	
}

.inline-header {
	display: inline-block;
}

.menu {
    position: relative;
    float: left;
    left: 1%;
    top: 5%;

    width: 33.3%;
    height: 13em;

    font-size: 0.7em;
    
    padding: 0.5em;
}

.info {
    position: relative;
    float: left;

    width: 105%;
    height: 3.5em;

    border: 1px solid white;
}

.surname, .jobprofile {
    position: relative;
    float: left;
    left: 2.5%;
    top: 13%;

    font-size: 1.3em;
    font-family: monospace;
}

.links {
    position: relative;
    float: left;
    top: 24%;

    width: 105%;
    height: 6em;

    border: 1px solid white;
}

.website {
    position: relative;
    float: left;
    left: 3%;
    top: 13%;

    width: 100%;
    height: 3.1em;
    
    font-size: 1.25em;
    font-family: monospace;
}

.website a {
    color: gray;
    
    font-variant: small-caps;
    font-size: 1.25em;
}


.website a .custom_link {
    position: relative;
    float: right;
    right: 9%;
    top: 0.5%;
    width: 14%;
    
    font-size: 1em;
    
    letter-spacing: 0.2em;
    
    text-decoration: none;
}


.website a .custom_link:hover {
    color: #666666;
}

.avatar {
	position: relative;
	float: right;
	right: 2em;
    top: 4.7%;

	width: 55%;
	height: 20.25em;
	
    border: 1px solid white;
    background-size: 100% 20.25em;
	background-image: url('../../images/d_me.png');
    background-repeat: no-repeat;
}

.notSupported {
    position: relative;
    width: 32em;
    height: 3.75em;
    
    background-color: black;
    color: white;
    border-radius: 15px;
    
    font-family: "verdana helvetica sans-serif";
    font-size: 1.1em;
    
    margin-left: auto;
    margin-right: auto;
    margin-top: 350px;
    
    padding-left: 1em;
    padding-right: 1em;
    padding-top: 1.15em;
    
    text-align: center;
}


.separator {
    height: 1em;
}

.frame {
    position: relative;
    top: -23%;
}

.content {
    position: relative;
}

.jedenProcent {
    position: relative;
    float: left;
    left: 3%;
    
    width: 32%;
    height: 7em;

    box-shadow: 10px 10px 5px #888888, -2px -2px 5px #888888;
    
    font-family: verdana helvetica sans-serif;
    
    padding-left: 2%;
    padding-top: 2%;
}

.jedenProcent .jp_info a:link, .jedenProcent .jp_details a:link {
    color: #155266;
}

.jedenProcent .jp_info a:hover, .jedenProcent .jp_details a:hover {
    color: #bd0013;
}

.jedenProcent .jp_info .procentSign {
    position: relative;
    
    color: #bd0013;
    font-size: xx-large;
}